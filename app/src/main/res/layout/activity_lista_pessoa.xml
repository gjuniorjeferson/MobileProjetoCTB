<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="adapter.android.dominando.mobileprojetoctb.cadastros.Pessoa.ListaPessoaActivity">

    <Button
        android:id="@+id/btnIncluirPessoa"
        android:layout_width="380dp"
        android:layout_height="50dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="1dp"
        android:layout_marginStart="3dp"
        android:text="@string/cadastrarPessoa"
        app:layout_constraintBottom_toTopOf="@+id/btnConsultarPessoa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnConsultarPessoa"
        android:layout_width="380dp"
        android:layout_height="50dp"
        android:layout_marginBottom="374dp"
        android:text="@string/consultarPessoa"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />
</android.support.constraint.ConstraintLayout>
